<style>
  div {
    width: 200px;
    height: 200px;
    border-radius: 18px;
    border: 8px black solid ;
    background-color: violet;
  }
  /*
  圆角分开设置*/
  .box{
    width: 200px;
    height: 200px;
    background-color: violet;
    border-top-left-radius: 5px;
    border-top-right-radius: 10px;
    border-bottom-left-radius: 20px;
    border-bottom-right-radius: 15px;
}
</style>
</head>

<body>
  <!-- 定义一个div标签 -->
  <div class="box"></div>
</body>

